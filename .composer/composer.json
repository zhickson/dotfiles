{
    "repositories": [
        {
            "name": "thecodeco/coding-standards",
            "type": "git",
            "url": "https://bitbucket.org/thecodeco/coding-standards/"
        }
    ],
    "require": {
        "thecodeco/coding-standards": "dev-main",
        "php": ">= 8.2",
        "spatie/global-ray": "^1.1",
        "laravel/installer": "^5.13"
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "^3.7",
        "wp-coding-standards/wpcs": "^3.0",
        "automattic/vipwpcs": "^3.0",
        "php-stubs/wordpress-stubs": "^6.4",
        "php-stubs/acf-pro-stubs": "^6.1",
        "php-stubs/wp-cli-stubs": "^2.9"
    },
    "config": {
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    }
}
